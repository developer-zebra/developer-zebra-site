parm:
  pivotal: No
  name: CellularDataUsage
  type: list
  list:
    - code: 0
      mx: "6.3"
      devicetype: A
      name: Do not change
      description: This value (or the absence of this parm from the XML) causes no change to CellularMgr settings; any previously selected setting is retained.
    - code: 1
      devicetype: A
      name: Enable
      mx: "6.3"
      description: Allows the device user to change the state (active/inactive) of cellular data on the device.
    - code: 2
      devicetype: A
      name: Disable
      mx: "6.3"
