			<parm name="LicenseChoice" value="zebra"/>
		<characteristic type="LicenseZebra">
			<parm name="LicenseActionZebra" value="activate"/>
		<characteristic type="NewLicenseZebra">
		<characteristic type="LicenseSourceURL">
