			<parm name="ActivationID" value="0e62-2e66-53ec-4619-86bb-5246-ce58-24d5"/>
			<parm name="ActivationQuantity" value="1"/>
		</characteristic>
		</characteristic>
		</characteristic>
	</wap-provisioningdoc>

