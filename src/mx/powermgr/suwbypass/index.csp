type: parm
order: 4
mx: "7.1"
devicetype: A
parm:
  pivotal: No
  name: SUWByPass
  type: list
  list:
    - code: 0
      mx: "7.1"
      devicetype: A
      name: Do Nothing
      description: This value (or the absence of this parm from the XML) causes no action on the device; the default value is used.
    - code: 1
      devicetype: A
      name: True
      mx: "7.1"
      description: Causes the Android device setup wizard to be skipped following an Enterprise Reset initiated through PowerMgr.  
    - code: 2
      devicetype: A
