parm:
  pivotal: Yes
  name: WakeUpIndividualAction
  type: list
  list:
    - code: 0
      mx: "9.2"
      osx: "7.7"
      devicetype: B
      name: Do not change
      description: This value (or the absence of this parm from the XML) causes no change to the device; any previously selected setting is retained.
    - code: 1
      devicetype: B
      name: Turn on
      mx: "9.2"
      osx: "7.7"
